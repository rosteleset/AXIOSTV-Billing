<div class='form-horizontal'>
  <div class='card box-info'>
    <div class='card-header with-border'>
      <h2 class='card-title'>_{INFO}_</h2>
    </div>
    <div class='card-body'>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='SESSION_ID'>_{SESSION_ID}_:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='SESSION_ID'
                 id='SESSION_ID' value='%SESSION_ID%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='START'>_{BEGIN}_:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='START' id='START'
                 value='%START%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='STOP'>_{END}_:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='STOP' id='STOP'
                 value='%STOP%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='DURATION'>_{DURATION}_:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='DURATION' id='DURATION'
                 value='%DURATION%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='TP_NAME'>_{TARIF_PLAN}_:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='TP_NAME' id='TP_NAME'
                 value='[%TP_ID%] %TP_NAME%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='IP'>IP:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='IP' id='IP'
                 value='%IP%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='CALLING_STATION_ID'>CALLING_STATION_ID (_{FROM}_):</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='CALLING_STATION_ID' id='CALLING_STATION_ID'
                 value='%CALLING_STATION_ID%'>
        </div>
      </div>
    </div>
  </div>

  <div class='card box-info '>
    <div class='card-header with-border'>
      <h2 class='card-title'>_{NAS}_</h2>
    </div>
    <div class='card-body'>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='NAS_ID'>ID:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='NAS_ID' id='NAS_ID'
                 value='%NAS_ID%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='NAS_NAME'>Name:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='NAS_NAME' id='NAS_NAME'
                 value='%NAS_NAME%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='NAS_IP'>IP:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='NAS_IP' id='NAS_IP'
                 value='%NAS_IP%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='SUM'>_{SUM}_:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='SUM' id='SUM'
                 value='%SUM%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='BILL_ID'>_{BILL}_:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='BILL_ID' id='BILL_ID'
                 value='%BILL_ID%'>
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element'>_{ROUTE}_:</label>
        <div class='col-md-9'>
          %ROUTE_ID%
        </div>
      </div>
      <div class='form-group row'>
        <label class='col-md-3 control-element' for='ACCT_TERMINATE_CAUSE'>_{ACCT_TERMINATE_CAUSE}_:</label>
        <div class='col-md-9'>
          <input disabled type='text' class='form-control' name='ACCT_TERMINATE_CAUSE' id='ACCT_TERMINATE_CAUSE'
                 value='%ACCT_TERMINATE_CAUSE%'>
        </div>
      </div>
    </div>
  </div>
</div>

<div class='col-md-6'>
  <div class='card card-%PANEL_TYPE%'>
    <div class='card-header card-title with-border text-center'>
      %FILE_NAME%
    </div>
    <div class='card-body'>
      <div class='form-group'>
        <label class='col-md-12'>_{COMMENTS}_</label>
        <div class='col-md-12'>%COMMENT%</div>
      </div>
      <div class='form-group'>
        <label class='col-md-12'>_{AMOUNT}_</label>
        <div class='col-md-12'>%AMOUNT%</div>
      </div>
      <div class='form-group'>
        <label class='col-md-12'>_{VERSION}_</label>
        <div class='col-md-12'>%VERSION%</div>
      </div>
      <div class='form-group'>
        <label class='col-md-12'>_{TIME_FOR_FILE}_</label>
        <div class='col-md-12'>%FILE_TIME%</div>
      </div>
      <div class='form-group'>
        <label class='col-md-12'>%DATE_TO%</label>
      </div>
      <div class='form-group'>
        <label class='col-md-12'>%AVAILABLE_AS_DEMO%</label>
      </div>
    </div>
    <div class='card-footer'>%BUTTON%</div>
  </div>
</div>

  "02:1:$lang{TABLES}:sqlcmd_tables:"      => 5,
  "03:1:$lang{TRIGGERS}:sqlcmd_triggers:"  => 5,
  "04:1:$lang{INFO}:sqlcmd_info:"          => 5,

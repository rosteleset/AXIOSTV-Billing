<div class='card card-primary card-outline'>
  <div class='card-header with-border'>
    <h4 class='card-title'>%TITLE% (%DATE%)</h4>
    <div class='card-tools float-right'>
      <button type='button' class='btn btn-tool' data-card-widget='collapse'><i class='fa fa-minus'></i></button>
    </div>
  </div>
  <div class='card-body'>
    <div class='row h-100'>
      <div class='col-4'>
        <div class='container h-100'>
          <div class='row align-items-center h-100'>
            <div class='col-12 text-center mx-auto'>
              <div class='image'>
                <img src='%ADMIN_AVATAR%' class='img-circle elevation-2 img-fluid'>
              </div>
              <div class='text-muted mt-2'>%ADMIN%</div>
            </div>
          </div>
        </div>
      </div>
      <div class='col-4 h-100 text-center'>
        <img src='/styles/default/img/modules/callcenter/arrows.png' class='h-100 d-inline-block'>
      </div>
      <div class='col-4'>
        <div class='container h-100'>
          <div class='row align-items-center h-100'>
            <div class='col-12 mx-auto'>
              <h3 class='text-primary text-bold ml-3 mb-0 h1'>%OUTGOING%</h3>
              <h1 class='text-warning text-bold ml-3 mb-0 title'>%TOTAL%</h1>
              <h3 class='text-success text-bold ml-3 mb-0 h1'>%INCOMING%</h3>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class='card card-outline %CARD_CLASS%'>
  <div class='card-header with-border'>
    <h4 class='card-title'>%HEADER%</h4>
    <div class='card-tools float-right'>
      <button type='button' class='btn btn-tool' data-card-widget='collapse'>
        <i class='fa fa-%COLLAPSE_ICON%'></i>
      </button>
    </div>
  </div>
  <div class='card-body'>
    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='SURNAME%INSURE_ID%'>_{FIO1}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='SURNAME%INSURE_ID%' name='SURNAME%INSURE_ID%' required
               value='%SURNAME%' class='form-control' type='text'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='NAME%INSURE_ID%'>_{FIO2}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='NAME%INSURE_ID%' name='NAME%INSURE_ID%' required value='%NAME%' class='form-control' type='text'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='PHONE%INSURE_ID%'>_{CELL_PHONE}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='PHONE%INSURE_ID%' name='PHONE%INSURE_ID%' value='%PHONE%' required
               class='form-control' type='text' autocomplete='off'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='EMAIL%INSURE_ID%'>E-mail:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='EMAIL%INSURE_ID%' name='EMAIL%INSURE_ID%' value='%EMAIL%' required class='form-control' type='text'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='BIRTHDAY%INSURE_ID%'>_{BIRTH_DATE}_:</label>
      <div class='col-sm-8 col-md-8'>
        %BIRTHDAY%
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='CITY%INSURE_ID%'>_{CITY}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='CITY%INSURE_ID%' name='CITY%INSURE_ID%' required value='%CITY%' class='form-control' type='text'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='STREET%INSURE_ID%'>_{STREET}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='STREET%INSURE_ID%' name='STREET%INSURE_ID%' required value='%STREET%' class='form-control' type='text'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label' for='BUILDING%INSURE_ID%'>_{BUILD}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='BUILDING%INSURE_ID%' name='BUILDING%INSURE_ID%' value='%BUILDING%' class='form-control' type='text'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label' for='FLAT%INSURE_ID%'>_{ADDRESS_FLAT}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='FLAT%INSURE_ID%' name='FLAT%INSURE_ID%' value='%FLAT%' class='form-control' type='text'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='PASSPORT_SERIES%INSURE_ID%'>_{ABON_PASSPORT_SERIES}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='PASSPORT_SERIES%INSURE_ID%' name='PASSPORT_SERIES%INSURE_ID%' required value='%PASSPORT_SERIES%' class='form-control' type='text'>
      </div>
    </div>

    <div class='form-group row'>
      <label class='col-sm-4 col-md-4 control-label required' for='PASSPORT_ID%INSURE_ID%'>_{ABON_PASSPORT_NUMBER}_:</label>
      <div class='col-sm-8 col-md-8'>
        <input id='PASSPORT_ID%INSURE_ID%' name='PASSPORT_ID%INSURE_ID%' required value='%PASSPORT_ID%' class='form-control' type='text'>
      </div>
    </div>
  </div>
</div>
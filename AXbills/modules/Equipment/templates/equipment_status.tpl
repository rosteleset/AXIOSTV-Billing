<div class='card card-info card-outline'>
  <div class='card-header with-border hidden-print text-center'>
    <i class='fas fa-chart-bar' style='font-size:18px; margin-right: 5px; float:left;'></i>
    <h4 class='card-title table-caption'>%TITLE%</h4>
    <div class='card-tools float-right'>
      <div class='btn-group'>
       <button type='button' title='Show/Hide' class='btn btn-tool' data-card-widget='collapse'>
       <i class='fa fa-minus'></i>
       </button>
      </div>
    </div>
  </div>
  <!-- /.box-header -->
  <div class='card-body'>
    <div class='row'>
      %CHARTS%
    </div>
    <!-- /.row -->
  </div>
  <!-- /.box-body -->
</div>


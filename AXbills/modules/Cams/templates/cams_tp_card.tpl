<div class='col-md-6'>
  <div class='card box-primary'>
    <div class='card-header text-center'>%NAME%</div>
    <div class='card-body'>
      <div class='form-group'>
        <label class='control-label col-md-6'>_{STREAMS_COUNT}_</label>
        <div class='col-md-6'>
          <p class='form-control-static'>%STREAMS_COUNT%</p>
        </div>
      </div>
      <div class='form-group'>
        <label class='control-label col-md-6'>_{PRICE}_</label>
        <div class='col-md-6'>
          <p class='form-control-static'>%PRICE%</p>
        </div>
      </div>
      <div class='form-group'>
        <label class='control-label col-md-6'>_{PAYMENT_TYPE}_</label>
        <div class='col-md-6'>
          <p class='form-control-static'>_{MONTH_FEE}_</p>
        </div>
      </div>
    </div>
    <div class='card-footer'>
      %SUBSCRIBE_BTN%
    </div>
  </div>
</div>

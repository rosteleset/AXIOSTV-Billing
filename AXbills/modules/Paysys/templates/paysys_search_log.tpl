<div class='col-xs-12 col-md-6'>
  <div class='card card-primary card-outline box-form form-horizontal'>
    <div class='card-body'>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='ID'>ID</label>
        <div class='col-md-9'>
          <input type=text id='ID' name='ID' value='%ID%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='SUM'>_{SUM}_</label>
        <div class='col-md-9'>
          <input type=text id='SUM' name='SUM' value='%SUM%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='BILL_ID'>_{BILL}_</label>
        <div class='col-md-9'>
          <input type=text id='BILL_ID' name='BILL_ID' value='%BILL_ID%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='PAY_SYSTEMS'>_{PAY_SYSTEM}_</label>
        <div class='col-md-9'>
          %PAY_SYSTEMS_SEL%
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='STATUS'>_{STATUS}_</label>
        <div class='col-md-9'>
          %STATUS_SEL%
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='REQUEST_TYPE'>_{REQUEST}_ _{METHOD}_</label>
        <div class='col-md-9'>
          %REQUEST_TYPE_SEL%
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='IP'>IP</label>
        <div class='col-md-9'>
          <input type=text id='IP' name='IP' value='%IP%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='TRANSACTION_ID'>_{TRANSACTION}_</label>
        <div class='col-md-9'>
          <input type=text id='TRANSACTION_ID' name='TRANSACTION_ID' value='%TRANSACTION_ID%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label' for='EXT_ID'>_{EXTERNAL_ID}_</label>
        <div class='col-md-9'>
          <input type=text id='EXT_ID' name='EXT_ID' value='%EXT_ID%' class='form-control'>
        </div>
      </div>

      <div class='form-group row'>
        <label class='col-md-3 col-form-label'>_{DATE}_</label>
        <div class='col-md-9'>
          %DATERANGE_PICKER%
        </div>
      </div>
    </div>
  </div>
</div>

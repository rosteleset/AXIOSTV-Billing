<div class='col-md-6 col-sm-12'>
  <div class='card card-primary card-outline'>
    <div class='card-body'>
      <div class='form-group'>
        <label class='control-label col-md-3'>_{NAME}_</label>
        <div class='col-md-9'>
          <input type='text' class='form-control' name='%ID%_NAME' value='%NAME%'/>
        </div>
      </div>
      <div class='form-group'>
        <label class='control-label col-md-3'>_{DESCRIBE}_</label>
        <div class='col-md-9'>
          <textarea type='text' class='form-control' name='%ID%_DESCRIBE'>%COMMENTS%</textarea>
        </div>
      </div>
      <div class='form-group'>
        <label class='control-label col-md-3'>_{ICON}_ URL</label>
        <div class='col-md-9'>
          <input type='text' class='form-control' name='%ID%_ICON'/>
        </div>
      </div>
      <div class='form-group'>
        <label class='control-label col-md-3'>_{DETAIL}_ URL</label>
        <div class='col-md-9'>
          <input type='text' class='form-control' name='%ID%_URL'/>
        </div>
      </div>
    </div>
  </div>
</div>

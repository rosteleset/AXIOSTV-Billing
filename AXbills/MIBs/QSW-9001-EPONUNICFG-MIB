    eponOnuPortMulticastOnuIndex                   INTEGER,
    eponOnuPortMulticastUniIndex                   INTEGER,
    eponOnuPortMulticastTag                        INTEGER (0..1),
    eponOnuPortMulticastIgmpMaxNum                 INTEGER (1..255),
    eponOnuPortMulticastIgmpVlanList               OCTET STRING (SIZE(0..255)),
    eponOnuPortMulticastIgmpVlanListRowStatus      RowStatus
    }


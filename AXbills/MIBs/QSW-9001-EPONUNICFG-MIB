   SYNTAX       SEQUENCE OF EponOnuEthPortCurrentStatisticsEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION 
     "onu eth pon port Current statistics  table"
   ::= {eponOnuPortStatisticsMgmt 3}

eponOnuEthPortCurrentStatisticsEntry  OBJECT-TYPE
   SYNTAX      EponOnuEthPortCurrentStatisticsEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   " onu eth port Current statistics entry"
   INDEX{eponOnuEthPortCurrentStatisticsCardIndex, eponOnuEthPortCurrentStatisticsPonIndex, eponOnuEthPortCurrentStatisticsOnuIndex,eponOnuEthPortCurrentStatisticsStatusOnuEthPortIndex}
   ::= {eponOnuEthPortCurrentStatisticsTable 1}
   
EponOnuEthPortCurrentStatisticsEntry ::= SEQUENCE{
    eponOnuEthPortCurrentStatisticsCardIndex              INTEGER,
    eponOnuEthPortCurrentStatisticsPonIndex               INTEGER,
    eponOnuEthPortCurrentStatisticsOnuIndex               INTEGER,
    eponOnuEthPortCurrentStatisticsStatusOnuEthPortIndex  INTEGER,	 
    eponOnuEthPortCurrentStatistics_dn_drop_events               Counter64,
    eponOnuEthPortCurrentStatistics_up_drop_events               Counter64,
    eponOnuEthPortCurrentStatistics_dn_octets                    Counter64,
    eponOnuEthPortCurrentStatistics_up_octets                    Counter64,
    eponOnuEthPortCurrentStatistics_dn_frames                    Counter64,
    eponOnuEthPortCurrentStatistics_up_frames   		         Counter64,
    eponOnuEthPortCurrentStatistics_dn_bc_frames                 Counter64,
    eponOnuEthPortCurrentStatistics_up_bc_frames                 Counter64,
    eponOnuEthPortCurrentStatistics_dn_mc_frames                 Counter64,
    eponOnuEthPortCurrentStatistics_up_mc_frames                 Counter64,
    eponOnuEthPortCurrentStatistics_dn_crc_error_frames          Counter64,
    eponOnuEthPortCurrentStatistics_up_crc_error_frames          Counter64,
    eponOnuEthPortCurrentStatistics_dn_undersize_frames          Counter64,
    eponOnuEthPortCurrentStatistics_up_undersize_frames          Counter64,
	eponOnuEthPortCurrentStatistics_dn_oversize_frames           Counter64,
	eponOnuEthPortCurrentStatistics_up_oversize_frames           Counter64, 
	eponOnuEthPortCurrentStatistics_dn_fragments                 Counter64,
	eponOnuEthPortCurrentStatistics_up_fragments                 Counter64,
	eponOnuEthPortCurrentStatistics_dn_jabbers                   Counter64,
	eponOnuEthPortCurrentStatistics_up_jabbers                   Counter64,

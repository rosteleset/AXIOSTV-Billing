eponOnuPortMulticastCtrlTable   OBJECT-TYPE
   SYNTAX        SEQUENCE OF EponOnuPortMulticastCtrlEntry
   MAX-ACCESS    not-accessible
   STATUS        current
   DESCRIPTION
   "onu port ctc multicast control"
   ::= {eponOnuPortMulticastMgmt 2}

eponOnuPortMulticastCtrlEntry  OBJECT-TYPE
   SYNTAX      EponOnuPortMulticastCtrlEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   " onu port multicast control info"
   INDEX{eponOnuPortMulticastCtrlCardIndex, eponOnuPortMulticastCtrlPonIndex, eponOnuPortMulticastCtrlOnuIndex, eponOnuPortMulticastCtrlUniIndex, eponOnuPortMulticastCtrlIndex}
   ::= {eponOnuPortMulticastCtrlTable 2}
   
EponOnuPortMulticastCtrlEntry ::= SEQUENCE{
    eponOnuPortMulticastCtrlCardIndex          INTEGER,
    eponOnuPortMulticastCtrlPonIndex           INTEGER,
    eponOnuPortMulticastCtrlOnuIndex           INTEGER,
    eponOnuPortMulticastCtrlUniIndex           INTEGER,
    eponOnuPortMulticastCtrlIndex              INTEGER (0..15),
    eponOnuPortMulticastCtrlType               INTEGER (0..2),
    eponOnuPortMulticastCtrlMac                MacAddress,
    eponOnuPortMulticastCtrlVlan               INTEGER (1..4094),
    eponOnuPortMulticastCtrlRowStatus          RowStatus
    }


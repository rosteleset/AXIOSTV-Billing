		eponOnuPortClassfPrecedenceIndex      INTEGER (1..8),
		eponOnuPortClassfQuemapped		        INTEGER (0..7),
		eponOnuPortClassfEthPriority	        INTEGER (0..7),
		eponOnuPortClassfDestIp		            IpAddress,
		eponOnuPortClassfDestMac		          MacAddress,
		eponOnuPortClassfDestPort		          INTEGER,
		eponOnuPortClassfDot1qPriority	      INTEGER (0..63),
		eponOnuPortClassfDscp		              INTEGER (0..63),
		eponOnuPortClassfEthType		          INTEGER,
		eponOnuPortClassfIpProtocol		        INTEGER (0..255),
		eponOnuPortClassfIpv6Precedence	      INTEGER (0..63),
		eponOnuPortClassfSrcIp		            IpAddress,
		eponOnuPortClassfSrcMac		            MacAddress,
		eponOnuPortClassfSrcPort		          INTEGER,
		eponOnuPortClassfVlan		              INTEGER,
		eponOnuPortClassfRowStatus		        RowStatus
    }   


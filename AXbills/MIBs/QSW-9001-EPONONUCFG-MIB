   "pon index"
   ::= {eponOnuCap2InterfaceTypeEntry 2}         

eponOnuCap2InterfaceOnuIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "onu index"
   ::= {eponOnuCap2InterfaceTypeEntry 3}  

eponOnuCap2InterfaceTypeIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "interfaceType index"
   ::= {eponOnuCap2InterfaceTypeEntry 4}  
   
eponOnuCap2InterfaceType  OBJECT-TYPE
  SYNTAX      INTEGER
  MAX-ACCESS  read-only
  STATUS      current
  DESCRIPTION " interface type
    0x00000000表示千兆以太网GE接口
    0x00000001表示百兆以太网FE接口（最高速率为100M，不含GE口）
    0x00000002表示VoIP接口
    0x00000003表示支持TDM 接口
    0x00000004表示支持ADSL2+接口
    0x00000005表示VDSL2接口
    0x00000006表示WLAN
    0x00000007表示USB口 
    0x00000008表示CATV RF口
    其他待定 "
  ::= {eponOnuCap2InterfaceTypeEntry 5}
  
eponOnuCap2NumOfPorts OBJECT-TYPE
  SYNTAX      INTEGER
  MAX-ACCESS  read-only
  STATUS      current
  DESCRIPTION 
  "number of interface"
  ::= {eponOnuCap2InterfaceTypeEntry 6}
  

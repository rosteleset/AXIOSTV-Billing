     eponOnuSlaUsCir           INTEGER,
     eponOnuSlaUsPir           INTEGER,
     eponOnuSlaUsWeight        INTEGER,
     eponOnuSlaDsPir           INTEGER,
     eponOnuSlaDsBurst         INTEGER,
    }


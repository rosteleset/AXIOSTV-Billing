   " onu queue sla"
   INDEX{eponOnuQueueSlaCardIndex, eponOnuQueueSlaPonIndex, eponOnuQueueSlaOnuIndex}
   ::= {eponOnuQueueSlaTable 1}

EponOnuQueueSlaEntry ::= SEQUENCE{
    eponOnuQueueSlaCardIndex             INTEGER,
    eponOnuQueueSlaPonIndex              INTEGER,
    eponOnuQueueSlaOnuIndex              INTEGER,
    eponOnuQueueSlaOperation             INTEGER,
    eponOnuQueueSlaSchedulingScheme      INTEGER,
    eponOnuQueueSlaPriBoundary           INTEGER,
    eponOnuQueueSlaCycleLength           INTEGER,
    eponOnuQueueSlaServiceNum            INTEGER
    }
    

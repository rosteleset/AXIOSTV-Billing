   DESCRIPTION
   "pon index"
   ::= {eponOnuQueueSlaEntry 2}     

eponOnuQueueSlaOnuIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "onu id"
   ::= {eponOnuQueueSlaEntry 3}  
   
eponOnuQueueSlaOperation  OBJECT-TYPE
   SYNTAX          INTEGER
   {
      deactivate(0),
      active(1)
   }
   MAX-ACCESS      read-write
   STATUS          current
   DESCRIPTION
   "Operation of ServiceDBA"
   ::= {eponOnuQueueSlaEntry 4} 

eponOnuQueueSlaSchedulingScheme  OBJECT-TYPE
   SYNTAX          INTEGER
   {
      SP(0),
      WRR(1),
      SP_WRR(2)
   }

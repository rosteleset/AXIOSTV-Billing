eponOnuOpmTable  OBJECT-TYPE
   SYNTAX       SEQUENCE OF EponOnuOpmEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION
    "onu opm diagnosis"
   ::= {eponOnuCfg 3}

eponOnuOpmEntry OBJECT-TYPE
   SYNTAX       EponOnuOpmEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION
    "onu opm diagnosis "
   INDEX{eponOnuOpmCardIndex, eponOnuOpmPonIndex, eponOnuOpmOnuIndex}
   ::= {eponOnuOpmTable 1}
   
EponOnuOpmEntry ::= SEQUENCE{
    eponOnuOpmCardIndex       INTEGER,
    eponOnuOpmPonIndex        INTEGER,
    eponOnuOpmOnuIndex        INTEGER,
    eponOnuOpmTemperature     OCTET STRING,
    eponOnuOpmVcc             OCTET STRING,
    eponOnuOpmBias            OCTET STRING,
    eponOnuOpmTXPower         OCTET STRING,
    eponOnuOpmRxPower         OCTET STRING,
    eponOnuOpmRefresh         INTEGER
    }


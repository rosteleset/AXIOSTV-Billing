eponOnuOpmAlarmThresholdTXPowerHighAlarm     OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " onu发送功率高告警门限值，最大取值：6.5535毫瓦，以16位无符号整数表示（0～65535），单位为0.1微瓦（uW），表示范围为0～6.5535毫瓦（大约为-40dBm～+8.2 dBm）"
    ::= { eponOnuOpmAlarmThresholdEntry 16}

eponOnuOpmAlarmThresholdTXPowerLowAlarm       OBJECT-TYPE
    SYNTAX      INTEGER 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " onu发送功率低告警门限值，最小取值：0毫瓦"
    ::= { eponOnuOpmAlarmThresholdEntry 17}

eponOnuOpmAlarmThresholdTXPowerHighWarning       OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " onu发送功率高警示门限值"
    ::= { eponOnuOpmAlarmThresholdEntry 18}

eponOnuOpmAlarmThresholdTXPowerLowWarning       OBJECT-TYPE
    SYNTAX      INTEGER 
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " onu发送功率低警示门限值"
    ::= { eponOnuOpmAlarmThresholdEntry 19}  

eponOnuOpmAlarmThresholdRXPowerHighAlarm      OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " onu接收功率高告警门限值，最大值：6.5535毫瓦，为ONU接受到的平均光功率，以16位无符号整数表示（0～65535），单位为0.1微瓦（uW），表示范围为0～6.5535毫瓦（大约为-40dBm～+8.2 dBm），在-30dBm到～10dBm"
    ::= { eponOnuOpmAlarmThresholdEntry 20}

eponOnuOpmAlarmThresholdRXPowerLowAlarm       OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " onu接收功率低告警门限值，最小值：0 毫瓦"
    ::= { eponOnuOpmAlarmThresholdEntry 21}

eponOnuOpmAlarmThresholdRXPowerHighWarning        OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " onu接收功率高警示门限值"
    ::= { eponOnuOpmAlarmThresholdEntry 22}
eponOnuOpmAlarmThresholdRXPowerLowWarning       OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        " onu接收功率低警示门限值"
    ::= { eponOnuOpmAlarmThresholdEntry 23}

---------------------ctc 3.0-----------------------
eponOnuEventStatusTable OBJECT-TYPE   
   SYNTAX       SEQUENCE OF EponOnuEventStatusEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION 
     "onu event status"
   ::= {eponOnuEventMgmt 3}

eponOnuEventStatusEntry  OBJECT-TYPE
   SYNTAX      EponOnuEventStatusEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   " onu event status"
   INDEX{eponOnuEventStatusCardIndex, eponOnuEventStatusPonIndex, eponOnuEventStatusOnuIndex, eponOnuEventStatusAlarmIndex}
   ::= {eponOnuEventStatusTable 1}

EponOnuEventStatusEntry ::= SEQUENCE{
    eponOnuEventStatusCardIndex       INTEGER,
    eponOnuEventStatusPonIndex        INTEGER,
    eponOnuEventStatusOnuIndex        INTEGER,
    eponOnuEventStatusAlarmIndex      INTEGER,
    eponOnuEventStatusAlarmID         INTEGER,
    eponOnuEventStatusAlarmStatus     INTEGER
    }

eponOnuEventStatusCardIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "slot index"
   ::= {eponOnuEventStatusEntry 1}      
      
eponOnuEventStatusPonIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "pon index"
   ::= {eponOnuEventStatusEntry 2}         

eponOnuEventStatusOnuIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "onu index"
   ::= {eponOnuEventStatusEntry 3} 
   
eponOnuEventStatusAlarmIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "alarm index"
   ::= {eponOnuEventStatusEntry 4}
   
eponOnuEventStatusAlarmID  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-write
   STATUS          current
   DESCRIPTION
   "alarm ID"
   ::= {eponOnuEventStatusEntry 5}
   
eponOnuEventStatusAlarmStatus  OBJECT-TYPE
   SYNTAX          INTEGER
   {
      disable(1),
      enable(2)
   }
   MAX-ACCESS      read-write
   STATUS          current
   DESCRIPTION
   "alarm status"
   ::= {eponOnuEventStatusEntry 6}
      

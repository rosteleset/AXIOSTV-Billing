eponOnuAlarmConfigTable OBJECT-TYPE   
   SYNTAX       SEQUENCE OF EponOnuAlarmConfigEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION 
     "onu alarm config"
   ::= {eponOnuEventMgmt 1}

eponOnuAlarmConfigEntry  OBJECT-TYPE
   SYNTAX      EponOnuAlarmConfigEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   " onu event status"
   INDEX{eponOnuAlarmConfigCardIndex, eponOnuAlarmConfigPonIndex, eponOnuAlarmConfigOnuIndex}
   ::= {eponOnuAlarmConfigTable 1}

EponOnuAlarmConfigEntry ::= SEQUENCE{
    eponOnuAlarmConfigCardIndex       INTEGER,
    eponOnuAlarmConfigPonIndex        INTEGER,
    eponOnuAlarmConfigOnuIndex        INTEGER,
    eponOnuAlarmConfigOpm             INTEGER,
    eponOnuAlarmConfigLoopback        INTEGER    
    }

eponOnuAlarmConfigCardIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "slot index"
   ::= {eponOnuAlarmConfigEntry 1}      
      
eponOnuAlarmConfigPonIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "pon index"
   ::= {eponOnuAlarmConfigEntry 2}         

eponOnuAlarmConfigOnuIndex  OBJECT-TYPE
   SYNTAX          INTEGER
   MAX-ACCESS      read-only
   STATUS          current
   DESCRIPTION
   "onu index"
   ::= {eponOnuAlarmConfigEntry 3} 
   
eponOnuAlarmConfigOpm  OBJECT-TYPE
   SYNTAX          INTEGER   
   {
      disable(1),
      enable(2)
   }
   MAX-ACCESS      read-write
   STATUS          current
   DESCRIPTION
   "opm alarm config"
   ::= {eponOnuAlarmConfigEntry 4}
   
eponOnuAlarmConfigLoopback  OBJECT-TYPE
   SYNTAX          INTEGER    
   {
      disable(1),
      enable(2)
   }
   MAX-ACCESS      read-write
   STATUS          current
   DESCRIPTION
   "loopback alarm config"
   ::= {eponOnuAlarmConfigEntry 5}           
         
----------ONU OPM ALARM THRESHOLD--------------------------------------------------   
eponOnuOpmAlarmThresholdTable  OBJECT-TYPE
   SYNTAX       SEQUENCE OF EponOnuOpmAlarmThresholdEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION
    "onu event threshold"
   ::= {eponOnuEventMgmt 2}

eponOnuOpmAlarmThresholdEntry OBJECT-TYPE
   SYNTAX       EponOnuOpmAlarmThresholdEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION
    "onu event threshold"
   INDEX{eponOnuOpmAlarmThresholdCardIndex, eponOnuOpmAlarmThresholdPonIndex, eponOnuOpmAlarmThresholdOnuIndex} 
   ::= {eponOnuOpmAlarmThresholdTable 1}

EponOnuOpmAlarmThresholdEntry ::= SEQUENCE{
    eponOnuOpmAlarmThresholdCardIndex                 INTEGER,
    eponOnuOpmAlarmThresholdPonIndex                  INTEGER,
    eponOnuOpmAlarmThresholdOnuIndex                  INTEGER,
    eponOnuOpmAlarmThresholdTempHighAlarm             INTEGER,
    eponOnuOpmAlarmThresholdTempLowAlarm              INTEGER,
    eponOnuOpmAlarmThresholdTempHighWarning           INTEGER,
    eponOnuOpmAlarmThresholdTempLowWarning            INTEGER,
    eponOnuOpmAlarmThresholdVccHighAlarm              INTEGER,
    eponOnuOpmAlarmThresholdVccLowAlarm               INTEGER,
    eponOnuOpmAlarmThresholdVccHighWarning            INTEGER,
    eponOnuOpmAlarmThresholdVccLowWarning             INTEGER,
    eponOnuOpmAlarmThresholdBiasHighAlarm             INTEGER,
    eponOnuOpmAlarmThresholdBiasLowAlarm              INTEGER,
    eponOnuOpmAlarmThresholdBiasHighWarning           INTEGER,
    eponOnuOpmAlarmThresholdBiasLowWarning            INTEGER,
    eponOnuOpmAlarmThresholdTXPowerHighAlarm          INTEGER,
    eponOnuOpmAlarmThresholdTXPowerLowAlarm           INTEGER,
    eponOnuOpmAlarmThresholdTXPowerHighWarning        INTEGER,
    eponOnuOpmAlarmThresholdTXPowerLowWarning         INTEGER,
    eponOnuOpmAlarmThresholdRXPowerHighAlarm          INTEGER,
    eponOnuOpmAlarmThresholdRXPowerLowAlarm           INTEGER,
    eponOnuOpmAlarmThresholdRXPowerHighWarning        INTEGER,
    eponOnuOpmAlarmThresholdRXPowerLowWarning         INTEGER
    }


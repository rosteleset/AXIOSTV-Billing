    "
    ::= {eponProfileOnuUniCTCAggVlanEntry 7}

onuUniAggDstVlan  OBJECT-TYPE
    SYNTAX          INTEGER(1..4094)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni aggregation dst vlan
    "
    ::= {eponProfileOnuUniCTCAggVlanEntry 8}

onuUniAggSrcVlanList  OBJECT-TYPE
    SYNTAX          OCTET STRING (SIZE(1..64))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni aggregation src vlan list
    "
    ::= {eponProfileOnuUniCTCAggVlanEntry 9}
          
------------------------------------------------------------------
-- epon profile rule cfg table
------------------------------------------------------------------

eponProfileRuleCfgTable  OBJECT IDENTIFIER  ::=  { eponProfileCfg 4 }

------------------------------------------------------------------
-- epon profile rule table
------------------------------------------------------------------
eponProfileRuleTable  OBJECT-TYPE
    SYNTAX       SEQUENCE OF EponProfileRuleEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "
        uni port vlan table
    "
    ::= {eponProfileRuleCfgTable 1}

eponProfileRuleEntry  OBJECT-TYPE
    SYNTAX       EponProfileRuleEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "
        rule profile
    "
    INDEX
    {
        ruleIndex
    }
    ::= {eponProfileRuleTable 1}

EponProfileRuleEntry  ::=  SEQUENCE
    {
        ruleIndex                       INTEGER,
        ruleActice                      INTEGER,
        
        ruleType                        OCTET STRING,
        ruleStartMac                    MacAddress,
        ruleEndMac                      MacAddress,
        ruleSlot                        INTEGER,
        rulePon                         INTEGER,
        ruleBindLine                    OCTET STRING,
        ruleActive                      INTEGER
    }

ruleIndex  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
    "
        rule index
    "
    ::= {eponProfileRuleEntry 1}

ruleActice  OBJECT-TYPE
    SYNTAX          INTEGER
    {
        delete(1)
    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        delete rule profile
    "
    ::= {eponProfileRuleEntry 2}

ruleType  OBJECT-TYPE
    SYNTAX          OCTET STRING(SIZE(3..6))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        onu type:
        null,
        204s,
        204m,
        ....
        2401,
        other
    "
    ::= {eponProfileRuleEntry 3}

ruleStartMac  OBJECT-TYPE
    SYNTAX          MacAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        start mac of the specific range
    "
    ::= {eponProfileRuleEntry 4}

ruleEndMac  OBJECT-TYPE
    SYNTAX          MacAddress
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        end mac of the specific range
    "
    ::= {eponProfileRuleEntry 5}

ruleSlot  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        specific slot
    "
    ::= {eponProfileRuleEntry 6}

rulePon  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        specific pon
    "
    ::= {eponProfileRuleEntry 7}

ruleBindLine  OBJECT-TYPE
    SYNTAX          OCTET STRING(SIZE(1..64))
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        binding line
    "
    ::= {eponProfileRuleEntry 8}

ruleActive  OBJECT-TYPE
    SYNTAX          INTEGER
    {
        inactive(0),
        active(1)
    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        active status of rule profile
    "
    ::= {eponProfileRuleEntry 9}
    
    
------------------------------------------------------------------
-- epon onu unique table
------------------------------------------------------------------
eponOnuUniqueAttributeTable  OBJECT IDENTIFIER  ::=  { eponProfileCfg 5 }

eponOnuUniqueTable  OBJECT-TYPE
    SYNTAX       SEQUENCE OF EponOnuUniqueEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "
        onu unique attribute table
    "
    ::= {eponOnuUniqueAttributeTable 1}

eponOnuUniqueEntry  OBJECT-TYPE
    SYNTAX       EponOnuUniqueEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "
        onu unique attribute entry
    "
    INDEX
    {
        uniqueSlotIndex,
        uniquePonIndex,
        uniqueOnuInde
    }
    ::= {eponOnuUniqueTable 1}

EponOnuUniqueEntry  ::=  SEQUENCE
    {
        uniqueSlotIndex                 INTEGER,
        uniquePonIndex                  INTEGER,
        uniqueOnuInde                   INTEGER,
        uniqueAttribute                 INTEGER
    }

uniqueSlotIndex  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
    "
        slot index
    "
    ::= {eponOnuUniqueEntry 1}

uniquePonIndex  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
    "
        pon index
    "
    ::= {eponOnuUniqueEntry 2}

uniqueOnuInde  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
    "
        onu index
    "
    ::= {eponOnuUniqueEntry 3}

uniqueAttribute  OBJECT-TYPE
    SYNTAX          INTEGER
    {
        inactive(0),
        active(1)
    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        onu unique profile switch
 	    0 -- disable
 	    1 -- enable
    "
    ::= {eponOnuUniqueEntry 4}
    


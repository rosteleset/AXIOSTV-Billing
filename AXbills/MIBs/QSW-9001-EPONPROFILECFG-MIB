eponProfileLineCfgTable  OBJECT IDENTIFIER  ::=  { eponProfileCfg 2 }

------------------------------------------------------------------
-- epon profile line base table
------------------------------------------------------------------

eponProfileLineBaseTable  OBJECT-TYPE
    SYNTAX       SEQUENCE OF EponProfileLineBaseEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "
        profile line table
    "
    ::= {eponProfileLineCfgTable 1}

eponProfileLineBaseEntry  OBJECT-TYPE
    SYNTAX       EponProfileLineBaseEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    INDEX
    {
        lineIndex
    }
    ::= {eponProfileLineBaseTable 1}

EponProfileLineBaseEntry  ::=  SEQUENCE
    {
        lineIndex                       INTEGER,
        lineAction                      INTEGER,
        
        lineDbaUpFir                    INTEGER,
        lineDbaUpCir                    INTEGER,
        lineDbaUpPir                    INTEGER,
        lineDbaUpWeigth                 INTEGER,
        lineDbaUpBurst                  INTEGER,
        lineDbaDownFir                  INTEGER,
        lineDbaDownCir                  INTEGER,
        lineDbaDownPir                  INTEGER,
        lineDbaDownWeigth               INTEGER,
        lineDbaDownBurst                INTEGER,
        lineDbaShaper                   INTEGER,
        
        lineMaxEthernet                 INTEGER
    }

lineIndex  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
    "
        line profile index
    "
    ::= {eponProfileLineBaseEntry 1}

lineAction  OBJECT-TYPE
    SYNTAX          INTEGER
    {
        delete(1)
    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        line profile action:
        delete(1)
    "
    ::= {eponProfileLineBaseEntry 2}

lineDbaUpFir  OBJECT-TYPE
    SYNTAX          INTEGER(1..100000)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        fir of upstream
    "
    ::= {eponProfileLineBaseEntry 3}

lineDbaUpCir  OBJECT-TYPE
    SYNTAX          INTEGER(1..100000)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        cir of upstream
    "
    ::= {eponProfileLineBaseEntry 4}

lineDbaUpPir  OBJECT-TYPE
    SYNTAX          INTEGER(1..100000)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        pir of upstream
    "
    ::= {eponProfileLineBaseEntry 5}

lineDbaUpWeigth  OBJECT-TYPE
    SYNTAX          INTEGER(1..20)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        weigth
        8600 don't support
        5610 support
    "
    ::= {eponProfileLineBaseEntry 6}

lineDbaUpBurst  OBJECT-TYPE
    SYNTAX          INTEGER(128..16383)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        burst
        8600 don't support
        5610 don't support in upstream
    "
    ::= {eponProfileLineBaseEntry 7}

lineDbaDownFir  OBJECT-TYPE
    SYNTAX          INTEGER(1..100000)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        fir of downstream
        5610 don't support in downstream
    "
    ::= {eponProfileLineBaseEntry 8}

lineDbaDownCir  OBJECT-TYPE
    SYNTAX          INTEGER(1..100000)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        cir of downstream
        5610 don't support in downstream
    "
    ::= {eponProfileLineBaseEntry 9}

lineDbaDownPir  OBJECT-TYPE
    SYNTAX          INTEGER(1..100000)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        pir of downstream
    "
    ::= {eponProfileLineBaseEntry 10}

lineDbaDownWeigth  OBJECT-TYPE
    SYNTAX          INTEGER(1..20)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        weigth
        8600 don't support
        5610 don't support in downstream
    "
    ::= {eponProfileLineBaseEntry 11}

lineDbaDownBurst  OBJECT-TYPE
    SYNTAX          INTEGER(128..16383)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        burst
        8600 don't support
        5610 support
    "
    ::= {eponProfileLineBaseEntry 12}

lineDbaShaper  OBJECT-TYPE
    SYNTAX          INTEGER
    {
        disable(0),
        cir(1),
        pir(2)
    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        dba shaper mode
        5610 don't support
    "
    ::= {eponProfileLineBaseEntry 13}

lineMaxEthernet  OBJECT-TYPE
    SYNTAX          INTEGER(1..25)
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        support ethernet port num,
        0 -- step increasing by onu
        other -- step increasing by port
    "
    ::= {eponProfileLineBaseEntry 14}

------------------------------------------------------------------
-- epon profile line port limit table
------------------------------------------------------------------
eponProfileLineUniCTCLimitTable  OBJECT-TYPE
    SYNTAX       SEQUENCE OF EponProfileLineUniCTCLimitEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "
        uni port speed limit table
    "
    ::= {eponProfileLineCfgTable 2}

eponProfileLineUniCTCLimitEntry  OBJECT-TYPE
    SYNTAX       EponProfileLineUniCTCLimitEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "
        uni port speed limit info
    "
    INDEX
    {
        lineLimitIndex,
        lineLimitUniIndex
    }
    ::= {eponProfileLineUniCTCLimitTable 1}

EponProfileLineUniCTCLimitEntry  ::=  SEQUENCE
    {
        lineLimitIndex                  INTEGER,
        lineLimitUniIndex               INTEGER,
        lineUniIngressEnable            INTEGER,
        lineUniIngressCir               INTEGER,
        lineUniIngressCbs               INTEGER,
        lineUniIngressEbs               INTEGER,
        lineUniEgressEnable             INTEGER,
        lineUniEgressCir                INTEGER,
        lineUniEgressPir                INTEGER
    }

lineLimitIndex  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
    "
        Line index
    "
    ::= {eponProfileLineUniCTCLimitEntry 1}

lineLimitUniIndex  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-only
    STATUS          current
    DESCRIPTION
    "
        uni port index
    "
    ::= {eponProfileLineUniCTCLimitEntry 2}

lineUniIngressEnable  OBJECT-TYPE
    SYNTAX          INTEGER
    {
        disable(0),
        enable(1)
    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni port ingress enable
    "
    ::= {eponProfileLineUniCTCLimitEntry 3}

lineUniIngressCir  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni port ingress cir
    "
    ::= {eponProfileLineUniCTCLimitEntry 4}

lineUniIngressCbs  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni port ingress cbs
        the value must be greater than 1522 
    "
    ::= {eponProfileLineUniCTCLimitEntry 5}

lineUniIngressEbs  OBJECT-TYPE
    SYNTAX          INTEGER
    
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni port ingress ebs
        the value must be  less than or equal to 1522
    "
    ::= {eponProfileLineUniCTCLimitEntry 6}

lineUniEgressEnable  OBJECT-TYPE
    SYNTAX          INTEGER
    {
        disable(0),
        enable(1)
    }
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni port egress enable
    "
    ::= {eponProfileLineUniCTCLimitEntry 7}

lineUniEgressCir  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni port egress cir
    "
    ::= {eponProfileLineUniCTCLimitEntry 8}

lineUniEgressPir  OBJECT-TYPE
    SYNTAX          INTEGER
    MAX-ACCESS      read-write
    STATUS          current
    DESCRIPTION
    "
        uni port egress pir
    "
    ::= {eponProfileLineUniCTCLimitEntry 9}


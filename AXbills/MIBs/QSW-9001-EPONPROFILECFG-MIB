------------------------------------------------------------------
-- epon profile line vlan swap table
------------------------------------------------------------------
eponProfileLineVlanSwapTable OBJECT-TYPE
   SYNTAX   SEQUENCE OF eponProfileLineVlanSwapEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   "epon profile vlan swap info"
   ::= {eponProfileLineCfgTable 6}
   
eponProfileLineVlanSwapEntry OBJECT-TYPE
   SYNTAX      eponProfileLineVlanSwapEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   "vlan swap info"
   INDEX{
        lineVlanSwapLineIndex,
        lineVlanSwapRuleIndex
   }
   ::= {eponProfileLineVlanSwapTable 1}

eponProfileLineVlanSwapEntry ::= SEQUENCE{
   lineVlanSwapLineIndex                INTEGER,
   lineVlanSwapRuleIndex                INTEGER,
   lineVlanSwapSVlan                    INTEGER,
   lineVlanSwapEVlan                    INTEGER,
   lineVlanSwapNVlan                    INTEGER,
   lineVlanSwapActoin                   INTEGER,
   lineVlanSwapIngressVlan              INTEGER,
   lineVlanSwapEgressVlan               INTEGER,
   lineVlanSwapStepVlan                 INTEGER,
   lineVlanSwapRowStatus                INTEGER
   }       

lineVlanSwapLineIndex   OBJECT-TYPE
   SYNTAX       INTEGER
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "Line index"
   ::= {eponProfileLineVlanSwapEntry 1}   
   
lineVlanSwapRuleIndex   OBJECT-TYPE
   SYNTAX       INTEGER
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "rule index: 1-16"
   ::= {eponProfileLineVlanSwapEntry 2}      
   
lineVlanSwapSVlan   OBJECT-TYPE
   SYNTAX       INTEGER(1..4094)
   MAX-ACCESS   read-create
   STATUS       current
   DESCRIPTION
   "start vlan"
   ::= {eponProfileLineVlanSwapEntry 3}
   
lineVlanSwapEVlan   OBJECT-TYPE
   SYNTAX       INTEGER(1..4094)
   MAX-ACCESS   read-create
   STATUS       current
   DESCRIPTION
   "end vlan"
   ::= {eponProfileLineVlanSwapEntry 4}
   
lineVlanSwapNVlan   OBJECT-TYPE
   SYNTAX       INTEGER(1..4094)
   MAX-ACCESS   read-create
   STATUS       current
   DESCRIPTION
   "swap vlan"
   ::= {eponProfileLineVlanSwapEntry 5}

lineVlanSwapActoin       OBJECT-TYPE
   SYNTAX       INTEGER
   {
        push(1),
        pop(2),
        swap(3)
   }
   MAX-ACCESS   read-create
   STATUS       current
   DESCRIPTION
   "action 1:push 2:pop 3:swap"
   ::={eponProfileLineVlanSwapEntry 6} 

lineVlanSwapIngressVlan       OBJECT-TYPE
   SYNTAX       INTEGER
   {
        inner-vlan(0),
        top-vlan(1)
   }
   MAX-ACCESS   read-create
   STATUS       current
   DESCRIPTION
   "0:inner vlan 1: top vlan"
   ::={eponProfileLineVlanSwapEntry 7} 

lineVlanSwapEgressVlan       OBJECT-TYPE
   SYNTAX       INTEGER
   {
        inner-vlan(0),
        top-vlan(1)
   }
   MAX-ACCESS   read-create
   STATUS       current
   DESCRIPTION
   "0:inner vlan 1: top vlan"
   ::={eponProfileLineVlanSwapEntry 8} 

lineVlanSwapStepVlan       OBJECT-TYPE
   SYNTAX       INTEGER(1..10)
   MAX-ACCESS   read-create
   STATUS       current
   DESCRIPTION
   "step vlan: 1-10"
   ::={eponProfileLineVlanSwapEntry 9} 
            
lineVlanSwapRowStatus       OBJECT-TYPE
   SYNTAX       RowStatus
   MAX-ACCESS   read-create
   STATUS       current
   DESCRIPTION
   "rowStatus"
   ::={eponProfileLineVlanSwapEntry 10}     
   
------------------------------------------------------------------
-- epon profile line ctc mcast table
------------------------------------------------------------------ 
eponProfileLineCtcMcastMgmt  OBJECT-IDENTITY
   STATUS        current
   DESCRIPTION
   "onu ctc multicast"
   ::= {eponProfileLineCfgTable 7}

eponProfileLineCtcMcastTable OBJECT-TYPE
   SYNTAX   SEQUENCE OF eponProfileLineCtcMcastEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   "epon profile ctc mcast info"
   ::= {eponProfileLineCtcMcastMgmt 1}

eponProfileLineCtcMcastEntry OBJECT-TYPE
   SYNTAX      eponProfileLineCtcMcastEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   "ctc mcast mode&fastleave info"
   INDEX{
        lineCtcMcastLineIndex   
        }
   ::= {eponProfileLineCtcMcastTable 1}

eponProfileLineCtcMcastEntry ::= SEQUENCE{
   lineCtcMcastLineIndex                INTEGER,
   lineCtcMcastLineMode       			INTEGER,
   lineCtcMcastLineFastLeave        	INTEGER
   }       

lineCtcMcastLineIndex   OBJECT-TYPE
   SYNTAX       INTEGER
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "Line index"
   ::= {eponProfileLineCtcMcastEntry 1}   

lineCtcMcastLineMode  OBJECT-TYPE
   SYNTAX          INTEGER
   {
       igmpsnoop(0),
       multicast-ctrl(1),
       disable-igmpsnoop(2)
   }
   MAX-ACCESS      read-write
   STATUS          current
   DESCRIPTION
   "onu multicast mode"
   ::= {eponProfileLineCtcMcastEntry 2}  

lineCtcMcastLineFastLeave  OBJECT-TYPE
   SYNTAX          INTEGER
   {
       fastleave-deactivate(1),
       fastleave-activate(2)
   }
   MAX-ACCESS      read-write
   STATUS          current
   DESCRIPTION
   "onu multicast fastleave mode  0:disable 1:enable"
   ::= {eponProfileLineCtcMcastEntry 3} 


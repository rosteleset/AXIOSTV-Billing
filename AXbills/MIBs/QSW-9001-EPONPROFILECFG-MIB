------------------------------------------------------------------
--  epon profile glob attribute
------------------------------------------------------------------
eponProfileGlobAttribute  OBJECT-IDENTITY
    STATUS      current
    DESCRIPTION
    "profile glob attribute"
    ::= {eponProfileCfg 1}

profileGlobSwitch  OBJECT-TYPE
    SYNTAX      INTEGER
    {
        disable(0),
        enable(1)
    }
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
    "
        status of profile
    "
    ::= {eponProfileGlobAttribute 1}

profileReturnCode  OBJECT-TYPE
    SYNTAX      INTEGER
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
    "
        return code
    "
    ::= {eponProfileGlobAttribute 2}


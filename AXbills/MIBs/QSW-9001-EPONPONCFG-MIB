
EPON-PON-CONFIG-MIB DEFINITIONS ::= BEGIN

IMPORTS
    IpAddress, Unsigned32, Integer32, Counter32, TimeTicks, Gauge32, Counter64,
    MODULE-IDENTITY, OBJECT-IDENTITY, OBJECT-TYPE, enterprises     
      FROM SNMPv2-SMI     
    DateAndTime, MacAddress, TimeStamp, RowStatus, TruthValue, DisplayString, TEXTUAL-CONVENTION
      FROM SNMPv2-TC
    eponPonCfg  FROM  EPON-CONFIG-MIB  ;

--------------------------------------------------------------------------------
--  ponCfg[PONPORT属性管理项](1.3.6.1.4.1.27514.1.10.2)
-------------------------------------------------------------------------------- 

eponPonInfoTable  OBJECT-TYPE
	SYNTAX      SEQUENCE OF EponPonInfoEntry
	MAX-ACCESS  not-accessible
	STATUS      current
	DESCRIPTION
		"pon info table"
	::= {eponPonCfg 1}

eponPonInfoEntry	OBJECT-TYPE
	SYNTAX	 	  EponPonInfoEntry
	MAX-ACCESS  not-accessible
	STATUS		  current
	DESCRIPTION
		"PonInfoEntry table"
	INDEX	{ eponPonCardIndex, eponPonPortIndex }
	::= {eponPonInfoTable 1}

EponPonInfoEntry ::= SEQUENCE{ 
   eponPonCardIndex         INTEGER,
   eponPonPortIndex         INTEGER,
   eponPonAdminStatus       INTEGER,
   eponPonOperationStatus   INTEGER,
   eponPonAuthenMode        INTEGER,
   eponPonLaoDetedtMode     INTEGER
  }
  
eponPonCardIndex   OBJECT-TYPE
   SYNTAX      INTEGER
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
   " slot index"
   ::= {eponPonInfoEntry 1}
   
eponPonPortIndex   OBJECT-TYPE
   SYNTAX      INTEGER
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
   "pon port index"
   ::= {eponPonInfoEntry 2}
   
eponPonAdminStatus   OBJECT-TYPE
   SYNTAX      INTEGER
   {
       disable(1),
       enable(2)
   }
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
   "pon admin status"
   ::= {eponPonInfoEntry 3}

eponPonOperationStatus   OBJECT-TYPE
   SYNTAX      INTEGER
   {
       linkdown(1),
       linkup(2)
   }
   MAX-ACCESS  read-only
   STATUS      current
   DESCRIPTION
   "pon link status"
   ::= {eponPonInfoEntry 4}  
   
eponPonAuthenMode   OBJECT-TYPE
   SYNTAX      INTEGER
   {
      disable(0),
      whiteListAuth(1),
      blackListAuth(2),
      loidAuth(3),
      hybridAuth(4)
   }
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
   "onu auth mode"
   ::= {eponPonInfoEntry 5}  

eponPonLaoDetedtMode  OBJECT-TYPE
   SYNTAX      INTEGER
   {
       disable(1),
       enable(2)
   }
   MAX-ACCESS  read-write
   STATUS      current
   DESCRIPTION
   "laser always on detect mode"
   ::= {eponPonInfoEntry 6} 
   
----PON OPM TABLE--------------------------------------------------------------------------
eponPonOpmTable OBJECT-TYPE
   SYNTAX   SEQUENCE OF EponPonOpmEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   "pon opm info"
   ::= {eponPonCfg 2}
   
eponPonOpmEntry OBJECT-TYPE
   SYNTAX      EponPonOpmEntry
   MAX-ACCESS  not-accessible
   STATUS      current
   DESCRIPTION
   "pon opm info"
   INDEX{eponPonOpmCardIndex, eponPonOpmPortIndex}
   ::= {eponPonOpmTable 1}

EponPonOpmEntry ::= SEQUENCE{
   eponPonOpmCardIndex      INTEGER,
   eponPonOpmPortIndex      INTEGER,
   eponPonOpmTemperature    OCTET STRING,
   eponPonOpmVcc            OCTET STRING,
   eponPonOpmBias           OCTET STRING,
   eponPonOpmTXPower        OCTET STRING,
   eponPonOpmRxPower        OCTET STRING,
   eponPonOpmRefresh        INTEGER
  }       

eponPonOpmCardIndex   OBJECT-TYPE
   SYNTAX       INTEGER
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "slot index"
   ::= {eponPonOpmEntry 1}
   
eponPonOpmPortIndex   OBJECT-TYPE
   SYNTAX       INTEGER
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "pon port index"
   ::= {eponPonOpmEntry 2}
   
eponPonOpmTemperature   OBJECT-TYPE
   SYNTAX       OCTET STRING
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "temperature , centi C"
   ::= {eponPonOpmEntry 3}
   
eponPonOpmVcc   OBJECT-TYPE
   SYNTAX       OCTET STRING
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "voltage , centi V"
   ::= {eponPonOpmEntry 4}      
   
eponPonOpmBias   OBJECT-TYPE
   SYNTAX       OCTET STRING
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "basi , centi mA"
   ::= {eponPonOpmEntry 5}
   
eponPonOpmTXPower   OBJECT-TYPE
   SYNTAX       OCTET STRING
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "tx power , centi mw"
   ::= {eponPonOpmEntry 6}
   
eponPonOpmRxPower   OBJECT-TYPE
   SYNTAX       OCTET STRING
   MAX-ACCESS   read-only
   STATUS       current
   DESCRIPTION
   "rx power , centi mw"
   ::= {eponPonOpmEntry 7}

eponPonOpmRefresh   OBJECT-TYPE
   SYNTAX       INTEGER
   MAX-ACCESS   read-write
   STATUS       current
   DESCRIPTION
   "refresh opm"
   ::= {eponPonOpmEntry 8}  
   

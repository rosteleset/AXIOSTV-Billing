	::= {eponPonCfg 4}

eponPonClsEntry	OBJECT-TYPE
	SYNTAX	 	  EponPonClsEntry
	MAX-ACCESS  not-accessible
	STATUS		  current
	DESCRIPTION
		"PonClsEntry table"
	INDEX	{ eponPonClsCardIndex, eponPonClsPortIndex, eponPonClsRuleIndex, eponPonClsPortType }
	::= {eponPonClsTable 1}

EponPonClsEntry ::= SEQUENCE{ 
   eponPonClsCardIndex         INTEGER,
   eponPonClsPortIndex         INTEGER,
   eponPonClsRuleIndex         INTEGER,
   eponPonClsPortType          INTEGER,
   eponPonClsRuleOpr           INTEGER,
   eponPonClsPermit            INTEGER,
   eponPonClsMatchMac          INTEGER,
   eponPonClsDstMac            MacAddress,
   eponPonClsSrcMac            MacAddress,
   eponPonClsMatchVlan         INTEGER,
   eponPonClsTVid              INTEGER,
   eponPonClsIVid              INTEGER,
   eponPonClsTop8021p          INTEGER,
   eponPonClsMatch8021p        INTEGER,
   eponPonClsInner8021p        INTEGER,
   eponPonClsMatchEthType      INTEGER,
   eponPonClsMatchDscp         INTEGER,
   eponPonClsMatchIP           INTEGER,
   eponPonClsDstIP             IpAddress,       
   eponPonClsSrcIP             IpAddress,
   eponPonClsMatchProtocol     INTEGER,
   eponPonClsMatchL4port       INTEGER,
   eponPonClsDstL4porthi       INTEGER, 
   eponPonClsDstL4portlo       INTEGER,
   eponPonClsSrcL4porthi       INTEGER, 
   eponPonClsSrcL4portlo       INTEGER,
   eponPonClsActTVlan          INTEGER,
   eponPonClsActIVlan          INTEGER,
   eponPonClsActVid            INTEGER,
   eponPonClsActDscp           INTEGER,    
   eponPonClsAct8021P          INTEGER,
   eponPonClsActCos            INTEGER,
   eponPonClsRowStatus         INTEGER
  }
  
